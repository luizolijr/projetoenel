    </head>
    
    <body class="body-sign-in">
    <div class="container">
        <div class="panel panel-default form-container">
            <div class="panel-body">
                {% if error %}
                    <div align="center"><br>{{ error.messageKey|trans(error.messageData, 'security') }}</div>
                {% endif %}
                <form method="post">
                    <h3 class="text-center margin-xl-bottom">Bem-vindo!</h3>

                    <div class="form-group text-center">
                        <label class="sr-only" for="email">Email</label>
                        <input name="email" type="email" class="form-control input-lg" id="email" placeholder="Email Address">
                    </div>
                    <div class="form-group text-center">
                        <label class="sr-only" for="password">Senha</label>
                        <input name="password" type="password" class="form-control input-lg" id="password" placeholder="Password">
                    </div>
                    <input type="hidden" name="_csrf_token" value="{{ csrf_token('authenticate') }}">

                    <button type="submit" class="btn btn-primary btn-block btn-lg">ENTRAR</button>
                    
                </form>
            </div>            
        </div>
    </div>
    {% block javascripts %}
        <script src="{{ asset('template-back/dist/assets/libs/jquery/jquery.min.js') }}"></script>
        <script src="{{ asset('template-back/dist/assets/bs3/js/bootstrap.min.js') }}"></script>
