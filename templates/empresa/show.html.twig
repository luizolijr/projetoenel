{% extends 'base.html.twig' %}

{% block title %}Empresa{% endblock %}

{% block body %}
    <div class="page-content">
            <div class="page-subheading page-subheading-md">
                <ol class="breadcrumb">
                    <li class="active">
                        <a href="javascript:;">Empresa</a>
                    </li>
                </ol>
            </div>

        <div class="container-fluid-md">
            <a class="btn btn-inverse" href="{{ path('empresa_index') }}">Voltar</a>            

            <a class=" btn btn-primary" href="{{ path('empresa_edit', {'id': empresa.id}) }}">Editar</a>            
            <br></br>
            {{ include('empresa/_delete_form.html.twig') }}

            

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ empresa.id }}</td>
